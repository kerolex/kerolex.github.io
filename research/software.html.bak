<!-- JavaScript for citation toggle functionality -->
	<script>
		document.getElementById('currentYear').textContent = new Date().getFullYear();
		
		// Toggle citation box visibility
		function toggleCitation(button, citationId) {
			const citationBox = document.getElementById(citationId);
			const isHidden = citationBox.style.display === 'none';
			
			// Hide all other citations
			document.querySelectorAll('.citation-box').forEach(box => {
				box.style.display = 'none';
			});
			document.querySelectorAll('.software-card .blog-post-link').forEach(btn => {
				if (btn !== button && btn.innerHTML.includes('Quote')) {
					btn.classList.remove('active');
				}
			});
			
			// Toggle current citation
			citationBox.style.display = isHidden ? 'block' : 'none';
			button.classList.toggle('active', isHidden);
		}
	</script>
	
